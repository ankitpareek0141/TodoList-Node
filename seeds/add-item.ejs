<!DOCTYPE html>
<html lang="en">

<%- include('./partials/header.ejs') %>

<body>

    <%- include('./partials/navbar.ejs') %>

    <div class="container">
        <div class="row">
            <form class="col s12" action="/insertItem" method="POST">
                <div class="row">
                    <div class="input-field col s6">
                        <i class="material-icons prefix">add_circle</i>
                        <input id="icon_prefix" name="itemName" type="text" class="validate">
                        <label for="icon_prefix">Name</label>
                    </div>
                    <div class="input-field col s6">
                        <i class="material-icons prefix">monetization_on</i>
                        <input id="icon_telephone" name="itemAge" type="tel" class="validate">
                        <label for="icon_telephone">Age</label>
                    </div>
                </div>
                <button class="btn">Add to list</button>
            </form>
        </div>
    </div>

    <script src="./assets/js/connect.js"></script>
</body>

</html>